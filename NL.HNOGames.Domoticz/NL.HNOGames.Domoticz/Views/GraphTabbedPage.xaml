<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="NL.HNOGames.Domoticz.Views.GraphTabbedPage"
		     xmlns:resources="clr-namespace:NL.HNOGames.Domoticz.Resources;assembly=NL.HNOGames.Domoticz"
             xmlns:custcontrol="clr-namespace:NL.HNOGames.Domoticz.Controls;assembly=NL.HNOGames.Domoticz">
    <TabbedPage.ToolbarItems>
        <ToolbarItem Text="{resources:Translate filter}" Order="Primary" Activated="ToolbarItem_Activated">
            <ToolbarItem.Icon>
                <OnPlatform x:TypeArguments="FileImageSource"
                            iOS="ic_sort.png"
                            Android="ic_sort.png"
                            WinPhone="ic_sort.png" />
            </ToolbarItem.Icon>
        </ToolbarItem>
    </TabbedPage.ToolbarItems>
</TabbedPage>